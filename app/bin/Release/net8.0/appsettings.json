{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "CustomErrorHandling": {
    "UseDeveloperExceptionPage": true,
    "UseExceptionHandler": "/Error",
    "UseStatusCodePagesWithReExecute": "/Error/{0}",
    "UseHsts": true
  },
   "AllowedCountries": ["TR"]
}